xterm*background: #000000
xterm*foreground: #FF0000
xterm*faceName: gohufont:bold
xterm*termName: xterm-256color
xterm*saveLines: 20000

Xft.rgba:rgb
Xft.antialias:true
Xft.dpi: 96
Xft.hinting:true
Xft.hintstyle:hintslight

urxvt.background: [90]#1F1A1A
urxvt.foreground: #E0E0E0
urxvt.geometry: 94x40
urxvt.scrollstyle: plain
urxvt.scrollTtyOutput: false
urxvt.scrollWithBuffer: true
urxvt.scrollTtyKeypress: true
urxvt.loginShell: true
urxvt.font: xft:Droid Sans Mono
!letterSpace fixes super wide chars
urxvt.letterSpace: -1 
urxvt.termName: screen-256color
urxvt.saveLines: 20000
urxvt.perl-ext-common: default,tabbed
urxvt.cursorColor: red
urxvt.cursorBlink: true
URxvt.tabbed.tabbar-fg: -2
URxvt.tabbed.tabbar-bg: -1
URxvt.tabbed.tab-fg: -2
URxvt.tabbed.tab-bg: 2

!black
*color0:        #0a0a0b
*color8:        #0a0a0b
!red
!green
*color2:     #00bd06
*color10:    #00bd06
!yellow
*color3:     #DEC000
*color11:    #DEC000
!blue
*color4:        #0013A3
*color12:       #0013A3
!magenta
!cyan
*color14:       #0013A3
!white
*color7:        #ffffff
*color15:       #ffffff
